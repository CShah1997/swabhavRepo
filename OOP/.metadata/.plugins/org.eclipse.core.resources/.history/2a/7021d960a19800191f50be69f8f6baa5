package com.techlabs.game;

import com.techlabs.game.*;

public class Player {
	private String name;
	private Marker mark;
	
	public Player(String name, Mark mark) {
		this.name = name;
		this.mark = mark;
	}

	public String getName() {
		return name;
	}

	public Mark getMark() {
		return mark;
	}	
}
