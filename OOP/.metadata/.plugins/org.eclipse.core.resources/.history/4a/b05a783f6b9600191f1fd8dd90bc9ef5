package com.techlabs.lsp_voilation;

public class Square extends Rectangle {

	public Square(int side) {
		super(side, side);
	}
	
	@Override
	public void setWidth(int width) {
		this.width = width;
		this.height=width;
	}
	
	public void setHeight(int height) {
		this.height = height;
		this.width=height;
	}
}
